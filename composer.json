{
    "name": "mpyw/laravel-database-mock",
    "description": "Database Mocking Library which mocks PDO underlying Laravel Connection classes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "database", "mock", "mocking", "unit", "test", "illuminate", "query", "eloquent"],
    "autoload": {
        "psr-4": {
            "Mpyw\\LaravelDatabaseMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\LaravelDatabaseMock\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "illuminate/database": "^11.0 || ^12.0 || ^13.0",
        "illuminate/support": "^11.0 || ^12.0 || ^13.0",
        "mpyw/mockery-pdo": "0.0.1-alpha8@alpha"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "orchestra/testbench-core": ">=9.0",
        "phpunit/phpunit": ">=11.0",
        "nilportugues/sql-query-formatter": "^1.2.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\LaravelDatabaseMock\\DatabaseMockServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
